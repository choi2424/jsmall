<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jsmall.mapper.MemberMapper">
	
	<select id="idCheck" resultType="String">
		SELECT 
			MEMBER_ID 
		FROM 
			MEMBER_TABLE  
		WHERE 
			MEMBER_ID = #{member_id}
	</select>
</mapper>